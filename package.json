    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "husky": "7.0.4",
    "jest": "^27.4.7",
    "json-schema-to-ts": "^1.6.5",
    "lint-staged": "^12.1.7",
