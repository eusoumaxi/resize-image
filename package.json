{
  "name": "thumbnail-generator-api",
  "version": "1.0.0",
  "description": "Serverless thumbnail-generator-api ",
  "main": "serverless.ts",
  "scripts": {
    "start:watch": "nodemon --exec serverless offline --stage staging --httpPort=4000",
    "start": "serverless offline --httpPort=4000 --stage staging",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --coverage",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prettier": "prettier --write '*/**/*.{js,ts}'",
    "docs": "redoc-cli serve docs/openapi/openapi.yml",
    "docs:build": "redoc-cli build docs/openapi/openapi.yml",
    "docs:watch": "redoc-cli serve docs/openapi/openapi.yml --watch"
  },
  "dependencies": {
    "lambda-multipart-parser": "^1.0.1",
